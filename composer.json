{
    "name": "wulfheart/elommr",
    "description": "An PHP implementation of EloMMR by Aram Ebtekar and Paul Liu",
    "keywords": [
        "Wulfheart",
        "elommr"
    ],
    "homepage": "https://github.com/wulfheart/elommr",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Wulf",
            "email": "alex.f.wulf@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Wulfheart\\EloMMR\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wulfheart\\EloMMR\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
